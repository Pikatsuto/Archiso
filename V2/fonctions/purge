PURGE_ALL(){
rm -rf $DIR;
}

PURGE_ISO(){
rm -rf $DIR/iso;
}

PURGE_PROFIL(){
rm -rf $DIR/profil
}

PURGE_REPO(){
rm -rf $DIR/profil
}

PURGE_WORK(){
rm -rf $DIR/work
}
